.container{
  display:flex;
  flex-direction: column;
  width:210rpx;
}

.poster{
  margin-bottom: 15rpx;
}

/* 过长的截断变成省略号 */
.name{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
  font-size: 30rpx;
  color:#2f4248 ;
}

.rate-container{
  display: flex;
  flex-direction: row;
}
